    "\n",
    "class Circle():\n",
    "    \n",
    "    # Class attribute\n",
    "    pi =3.14\n",
    "    \n",
    "    # define instance of the class\n",
    "    def __init__ (self,radius=1):\n",
    "        self.radius = radius\n",
    "        \n",
    "    #Method of class\n",
    "    \n",
    "    def get_circumference(self):\n",
    "        return 2*self.pi*self.radius\n",
    "    \n",
    "    def get_area(self):\n",
    "        return Circle.pi*self.radius*self.radius\n",
    "    \n"
   ]
  },
