    "def return_33s(nums):\n",
    "    print(len(nums)-1)\n",
    "    for i in range(0,len(nums)-1):\n",
    "        print(nums[i:i+2])\n",
    "        if nums[i:i+2] == [3,3]:\n",
    "            print(nums[i:i+2])\n",
    "            return True\n",
    "    return False\n",
    "    "
   ]
  },
