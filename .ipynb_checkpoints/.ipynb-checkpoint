    "#summer_69(1,2,6,4,9,7)\n",
    "def summer_69(*args):\n",
    "    CanAdd = True\n",
    "    sum =0\n",
    "    for i in args:\n",
    "        if i == 6:\n",
    "            CanAdd = False\n",
    "        elif i ==9 and CanAdd == False:\n",
    "             CanAdd = True\n",
    "        elif CanAdd:\n",
    "            sum +=i\n",
    "    return sum\n",
    "            \n",
    "            \n",
    "    "
   ]
