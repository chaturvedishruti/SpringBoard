   "source": [
    "def repeatednums(array):\n",
    "    new = []\n",
    "    for i in range(0,len(array)):\n",
    "        for j in range(i+1,len(array)):\n",
    "            if array[i] == array[j]:\n",
    "                new.append(array[i])\n",
    "    return(set(new))            "
   ]
  },
