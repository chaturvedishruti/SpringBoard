    "def old_macdonald(str1):\n",
    "    result=\"\"\n",
    "    for index,letter in enumerate(str1):\n",
    "        if index ==0 or index ==3:\n",
    "             result += letter.upper()\n",
    "        else:\n",
    "            result += letter\n",
    "    return result            \n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "old_macdonald('OLDmacdonald')"
   ]
  },
