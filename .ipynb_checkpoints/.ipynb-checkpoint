    "  blackJackSum = sum([x,y,z])\n",
    "  if blackJackSum <= 21:\n",
    "    print(blackJackSum)\n",
    "  elif 11 in [x,y,z] and blackJackSum - 10 <= 21:\n",
    "    print(blackJackSum - 10)\n",
    "  else:\n",
    "    print('BUST')"
   ]
